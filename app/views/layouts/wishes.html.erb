<!DOCTYPE html>
<html>
<head>
  <title>Ohio Credit Flexibility</title>
  <%= stylesheet_link_tag    "application" %>
  <%= stylesheet_link_tag    "courses" %>
  <%= stylesheet_link_tag    "headers" %>
  <%= stylesheet_link_tag    "lycee" %>
  <%# javascript_include_tag :defaults %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= cache do %>
<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=376041366214";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
<%= render(:partial => "shared/header2") %>
<div class="clear"></div>
<% end %>
<%= yield %>

</body>
</html>
